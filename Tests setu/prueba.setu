	(pob[pobSize,pobLength])
	((for i 1:pobSize)
		((for j 1:pobLength)
			(random = Random())
			((if (random<0.5))
				(
				(pob(i,j) = 1)
				)
				(
				(pob(i,j) = 0)
				)
			)
		)
	)
)
